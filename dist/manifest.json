{
	"manifest_version": 2,
	"name": "__MSG_extExtension__",
	"version": "1.3",
	"description": "__MSG_extDescription__",
	"default_locale": "en",
	"icons": {
		"128": "icons/logo.png"
	},
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"downloads",
		"notifications",
		"storage",
		"<all_urls>"
	],
	"browser_action": {
		"default_popup": "popup.html",
		"browser_style": false
	}
}